{
  "name": "workbench",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --detectOpenHandles --verbose --config ./test/jest-e2e.json",
    "test:e2e:cov": "jest --detectOpenHandles --coverage --config ./test/jest-e2e.json",
    "migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate -d ./ormconfig.ts ./migrations",
    "migration:create": "ts-node ./node_modules/.bin/typeorm migration:create ./migrations",
    "migration:run": "ts-node ./node_modules/.bin/typeorm migration:run -d ./ormconfig.ts",
    "migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert -d ./ormconfig.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.621.0",
    "@aws-sdk/client-s3": "^3.621.0",
    "@aws-sdk/client-sqs": "^3.621.0",
    "@aws-sdk/lib-dynamodb": "^3.616.0",
    "@aws-sdk/s3-request-presigner": "^3.616.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.4.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/swagger": "^7.4.1",
    "@nestjs/typeorm": "^10.0.0",
    "@sendgrid/mail": "^8.1.3",
    "argon2": "^0.40.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csv-parse": "^5.5.5",
    "dayjs": "^1.11.10",
    "envalid": "^8.0.0",
    "handlebar": "^1.0.0",
    "humanloop": "^0.8.15",
    "install": "^0.13.0",
    "json2csv": "^6.0.0-alpha.2",
    "libphonenumber-js": "^1.11.13",
    "mongodb": "^5.8.1",
    "nest-winston": "^1.9.4",
    "newrelic": "^11.23.2",
    "node-fetch": "^3.3.2",
    "npm": "^10.5.2",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfmake": "^0.2.7",
    "pg": "^8.11.3",
    "pusher": "^5.2.0",
    "redoc-express": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "twilio": "^5.3.5",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "../jest.setup.js"
    ]
  }
}
